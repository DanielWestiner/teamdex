<div class='row justify-space-between-md'>
  <form>
  <div class="form-group">
    <label for="email-login">Email address</label>
    <input type="email" class="form-control" id="email-login" aria-describedby="emailHelp" placeholder="Enter your email">
  </div>
  <div class="form-group mt-3">
    <label for="login-password" class="mt-4">Password</label>
    <input type="password" class="form-control mb-4" id="password-login" placeholder="Password">
  </div>
  <a href="/signup">New to TeamDex? Sign up here</a>
  <br>
  <button class="btn btn-primary mt-4" id="login-btn">Submit</button>
</form>
</div>

<script src='./js/login.js'></script>
